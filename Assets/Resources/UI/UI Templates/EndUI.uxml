    <Style src="SettingsMenuUI.uss" />
    <ui:VisualElement name="screen" style="flex-grow: 1; background-color: rgba(0, 0, 0, 0.51); justify-content: center; align-items: center;">
        <ui:VisualElement name="top">
            <ui:Label name="complete-label" style="background-image: resource(&apos;UI/Texts/Complete&apos;); width: 1095px; height: 532px; margin-top: 265px;" />
